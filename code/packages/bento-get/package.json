{
  "name": "bento-get",
  "version": "1.102.3",
  "license": "MIT",
  "bin": "dist/cli.js",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public"
  },
  "source": "src/cli.tsx",
  "types": "./types/cli.d.ts",
  "module": "dist",
  "files": [
    "source",
    "types",
    "dist"
  ],
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "lint": "npx biome check src",
    "lint:fix": "npx biome check --apply-unsafe src",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build",
    "local-npm-link": "npm link --local --force"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "react-native": "./dist/cjs/index.native.js",
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    }
  },
  "dependencies": {
    "@inkjs/ui": "^1.0.0",
    "@supabase/supabase-js": "^2.44.3",
    "@tamagui/use-force-update": "1.102.3",
    "conf": "^12.0.0",
    "copy-paste": "^1.5.3",
    "fuse.js": "^7.0.0",
    "ink": "^4.1.0",
    "ink-link": "^3.0.0",
    "ink-text-input": "^5.0.1",
    "node-fetch": "^3.3.2",
    "octokit": "^4.0.2",
    "open": "^10.1.0",
    "pastel": "^2.0.0",
    "react": "^18.2.0",
    "swr": "^2.2.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tamagui/build": "1.102.3",
    "@types/copy-paste": "^1.1.33",
    "@types/react": "^18.2.55"
  }
}
